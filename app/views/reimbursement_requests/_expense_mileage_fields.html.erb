<div class="form-row nested-fields">
  <div class="form-col"><%=f.input :from_date, as: :date_picker, label: false %></div>
  <div class="form-col"><%= f.input :to_date, as: :date_picker, label: false %></div>
  <div class="form-col"><%= f.select :from_state, 
    CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] },
    { prompt: "State" },
    id: "states-of-country", class: "form-control"  %></div>
  <div class="form-col"><%= f.select :from_city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></div>
  <div class="form-col"><%= f.select :to_state, 
    CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] },
    { prompt: "State" },
    id: "states-of-country", class: "form-control"  %></div>
  <div class="form-col"><%= f.select :to_city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></div>
  <div class="form-col"><%= f.input :round_trip, label: false %></div>
  <div class="form-col"><%= f.input :miles, label: false %></div>
  <div class="form-col"><%= f.input :notes, label: false, as: :string %></div>
  <div class="form-col"><%= f.input :amount, label: false, input_html: { class: "amount-input" } %></div>
  <div class="form-col"><%= link_to_remove_association "Remove", f %></div>
</div>
