
<tr class="nested-fields">
  <td><%=f.input :from_date, as: :date_picker, label: false %></td>
  <td><%= f.input :to_date, as: :date_picker, label: false %></td>
  <td>
    <%= f.select :from_state,
          CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] },
          { prompt: "State" },
          id: "states-of-country", class: "form-control"  %>
  </td>
  <td><%= f.select :from_city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></td>
  <td>
    <%= f.select :to_state,
        CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] },
        { prompt: "State" },
        id: "states-of-country", class: "form-control"  %>
  </td>
  <td><%= f.select :to_city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></td>
  <td><%= f.input :round_trip, label: false %></td>
  <td><%= f.input :miles, label: false %></td>
  <td><%= f.input :notes, label: false, as: :string %></td>
  <td><%= f.input :amount, label: false, input_html: { class: "amount-input" } %></td>
  <td>
    <%= link_to_remove_association f, class: "btn btn-danger btn-sm"  do %>
      <i class="fa fa-trash-o fa-2x"></i>
    <% end %></td>
</tr>