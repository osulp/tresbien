<div id="attachments_panel" class="form-row tab-pane">
  <div class="col col-12">
    <div class="form-row">
      <h3><%= t('reimbursement_request_form.headers.attachments')%></h3>
    </div>
    <div class="form-row">
    <div class="alert alert-info" role="alert">
      <strong>Please note</strong>
      <p>If you are a non resident alien, please attach your <strong>I-94</strong> to this reimbursement request.<br>
        Please email your business office for more details.
      </p>
      </div>
    </div>
    <div class="form-row">
      <%= f.input 'file_attachments', as: :file, label: false,  input_html:{multiple: true, class: "attachment"} %>
    </div>
  </div>
</div>
