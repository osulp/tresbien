<tr class="nested-fields">
  <td><%= f.input :date, as: :date_picker, label: false %></td>
  <td><%= f.select :state, CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] }, { prompt: "State" }, id: "states-of-country", class: "form-control"  %></td>
  <td><%= f.select :city, [],{ prompt: "City" }, id: "cities-of-state", class: "form-control" %></td>
  <td><%= f.input :break, label: false %></td>
  <td><%= f.input :lunch, label: false %></td>
  <td><%= f.input :dinner, label: false %></td>
  <td><%= f.input :hotel, label: false %></td>
  <td><%= f.input :amount, label: false, input_html: { class: "amount-input" } %></td>
  <td><%= link_to_remove_association "Remove", f %></td>
</tr>
