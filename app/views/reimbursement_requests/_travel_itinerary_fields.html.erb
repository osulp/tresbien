<div class="form-row nested-fields">
  <div class="form-col"><%= f.input :date, as: :date_picker, label: false %></div>
  <div class="form-col"><%= f.select :state, 
    CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] },
    { prompt: "State" },
    id: "states-of-country", class: "form-control"  %></div>
  <div class="form-col"><%= f.select :city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></div>
  <div class="form-col"><%= f.input :break, label: false %></div>
  <div class="form-col"><%= f.input :lunch, label: false %></div>
  <div class="form-col"><%= f.input :dinner, label: false %></div>
  <div class="form-col"><%= f.input :hotel, label: false %></div>
  <div class="form-col"><%= f.input :amount, label: false, input_html: { class: "amount-input" } %></div>
  <div class="form-col"><%= link_to_remove_association "Remove", f %></div>
</div>
