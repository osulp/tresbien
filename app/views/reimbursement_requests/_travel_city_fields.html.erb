

<tr class="nested-fields">
  <td><%= f.input :from_date, as: :date_time_picker, label: false %></td>
  <td><%= f.input :to_date, as: :date_time_picker, label: false %></td>
  <td><%= f.select :state, CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] }, { prompt: "State" }, id: "states-of-country", class: "form-control", label: false %></td>
  <td><%= f.select :city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></td>
  <td><%= f.input :include_meals, as: :boolean, wrapper_html: { class: "form-check" }, input_html: { class: "form-check" }, label: false %></td>
  <td><%= f.input :hotel_rate, label: false %></td>
  <td>
      <%= link_to_remove_association f, class: "btn btn-danger btn-sm"  do %>
        <i class="fa fa-trash-o fa-2x"></i>
      <% end %>
    </td>
</tr>