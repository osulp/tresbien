<div class="form-row nested-fields">
  <div class="form-col"><%= f.input :from_date, as: :date_picker, label: false %></div>
  <div class="form-col"><%= f.input :to_date, as: :date_picker, label: false %></div>
  <div class="form-col"><%= f.select :state, 
    CS.get(:us).map { |state| [state[1], state[1], { id: state[0] } ] },
    { prompt: "State" },
    id: "states-of-country", class: "form-control", label: false%></div>
  <div class="form-col"><%= f.select :city, [],{prompt: "City"}, id: "cities-of-state", class: "form-control" %></div>
  <div class="form-col"><%= f.input :include_meals, as: :boolean, wrapper_html: { class: "form-check" }, input_html: { class: "form-check" }, label: false %></div>
  <div class="form-col"><%= f.input :hotel_rate,  input_html: { class: "amount-input" }, label: false %></div>
  <div class="form-col"><%= link_to_remove_association "Remove", f %></div>
</div>
